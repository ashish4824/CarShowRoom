<svg xmlns="http://www.w3.org/2000/svg" width="800" height="500" viewBox="0 0 800 500">
  <rect width="800" height="500" fill="#f0f0f0"/>
  <g transform="translate(400, 250)">
    <!-- Car body -->
    <path d="M-200,-50 L-170,-50 L-150,-20 L150,-20 L200,30 L200,60 L-200,60 Z" fill="#3498db" stroke="#2980b9" stroke-width="2"/>
    
    <!-- Car roof -->
    <path d="M-100,-20 L-70,-80 L70,-80 L100,-20 Z" fill="#2c3e50" stroke="#2c3e50" stroke-width="2"/>
    
    <!-- Windows -->
    <path d="M-95,-20 L-65,-75 L65,-75 L95,-20 Z" fill="#ecf0f1" stroke="#bdc3c7" stroke-width="1"/>
    <line x1="0" y1="-75" x2="0" y2="-20" stroke="#bdc3c7" stroke-width="1"/>
    
    <!-- Wheels -->
    <circle cx="-120" cy="60" r="40" fill="#34495e" stroke="#2c3e50" stroke-width="2"/>
    <circle cx="-120" cy="60" r="25" fill="#95a5a6" stroke="#7f8c8d" stroke-width="2"/>
    <circle cx="120" cy="60" r="40" fill="#34495e" stroke="#2c3e50" stroke-width="2"/>
    <circle cx="120" cy="60" r="25" fill="#95a5a6" stroke="#7f8c8d" stroke-width="2"/>
    
    <!-- Headlights -->
    <rect x="-200" y="0" width="20" height="20" rx="5" fill="#f1c40f" stroke="#f39c12" stroke-width="1"/>
    <rect x="180" y="0" width="20" height="20" rx="5" fill="#e74c3c" stroke="#c0392b" stroke-width="1"/>
    
    <!-- Grill -->
    <rect x="-160" y="20" width="40" height="20" rx="5" fill="#95a5a6" stroke="#7f8c8d" stroke-width="1"/>
  </g>
  
  <!-- Text -->
  <text x="400" y="400" font-family="Arial" font-size="24" text-anchor="middle" fill="#2c3e50">3D Car Model Placeholder</text>
  <text x="400" y="430" font-family="Arial" font-size="18" text-anchor="middle" fill="#7f8c8d">Unity WebGL will render here</text>
</svg>